kh/Desktop/2lzy/src/components/popup.tsx",["185","186"],"import React, { FC, memo, useMemo, useEffect } from \"react\";\nimport { List } from \"types/data\";\nimport { useDispatch } from \"react-redux\";\nimport { closePopup } from \"state/actions/popup\";\nimport { ListCard } from \"components/listCard\";\nimport { AddNew } from \"components/addNew\";\n\ninterface Props {\n  type?: \"list\" | \"addNew\";\n  item?: List;\n}\n\nexport const Popup: FC<Props> = memo(({ item, type }) => {\n  const dispatch = useDispatch();\n  const closeCurrentPopup = () => {\n    dispatch(closePopup());\n  };\n  useEffect(() => {\n    document.addEventListener(\"keydown\", closePopupByEsc, false);\n\n    return () => {\n      document.removeEventListener(\"keydown\", closePopupByEsc, false);\n    };\n  }, []);\n\n  const closePopupByEsc = (e: any) => {\n    if (!e) {\n      return;\n    }\n    if (e.key === \"Escape\") {\n      closeCurrentPopup();\n    }\n  };\n\n  const body = useMemo(() => {\n    switch (type) {\n      case \"list\":\n        if (!item) {\n          return null;\n        }\n        return <ListCard list={item} usage=\"popup\" />;\n      case \"addNew\":\n        return <AddNew />;\n    }\n  }, [type, item?.id]);\n\n  if (!type) {\n    return null;\n  }\n\n  return (\n    <div className=\"w-screen h-screen bg-black bg-opacity-80 absolute top-0 left-0 z-base flex items-center justify-center\">\n      <div className=\"w-2/5 h-2/3 bg-white rounded-md relative p-16\">\n        {body}\n      </div>\n    </div>\n  );\n});\n",["187","188"],"/Users/a.vodolazskikh/Desktop/2lzy/src/utils/generateUuid.ts",[],["189","190"],"/Users/a.vodolazskikh/Desktop/2lzy/src/state/selectors/popup.ts",[],["191","192"],"/Users/a.vodolazskikh/Desktop/2lzy/src/state/actions/popup.ts",[],"/Users/a.vodolazskikh/Desktop/2lzy/src/state/reducers/index.ts",[],"/Users/a.vodolazskikh/Desktop/2lzy/src/state/reducers/popup.ts",[],"/Users/a.vodolazskikh/Desktop/2lzy/src/components/listPreview.tsx",[],"/Use